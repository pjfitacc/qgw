{
  "name": "qgw",
  "version": "2.0.1",
  "description": "Typescript library with functions for the QuantumGateway Integration APIs: https://www.quantumgateway.com/developer.php",
  "types": "./dist/index.d.ts",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./transparent-db": {
      "import": {
        "types": "./dist/transparent-db/index.d.mts",
        "default": "./dist/transparent-db/index.mjs"
      },
      "require": {
        "types": "./dist/transparent-db/index.d.ts",
        "default": "./dist/transparent-db/index.js"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "test": "jest",
    "docs": "typedoc",
    "tempDocs": "typedoc --out tempDocs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pjfitacc/qgw.git"
  },
  "keywords": [
    "quantum-gateway",
    "payment-processing",
    "typescript",
    "library"
  ],
  "author": "Philippine Jesuit Foundation Developer",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/pjfitacc/qgw/issues"
  },
  "homepage": "https://github.com/pjfitacc/qgw#readme",
  "devDependencies": {
    "@eslint/js": "^9.25.1",
    "@types/jest": "^29.5.14",
    "eslint": "^9.25.1",
    "eslint-plugin-tsdoc": "^0.4.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.3.2",
    "ts-node": "^10.9.2",
    "tsup": "^8.4.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.31.1"
  },
  "dependencies": {
    "axios": "^1.9.0",
    "class-transformer": "^0.5.1",
    "reflect-metadata": "^0.2.2",
    "typedoc": "^0.28.3",
    "zod": "^3.24.3"
  }
}
